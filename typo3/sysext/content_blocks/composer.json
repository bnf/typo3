{
	"name": "typo3/cms-content-blocks",
	"type": "typo3-cms-framework",
	"description": "TYPO3 CMS Content Blocks - Content Types API",
	"homepage": "https://typo3.org",
	"license": ["GPL-2.0-or-later"],
	"authors": [{
		"name": "TYPO3 Content Types Team",
		"email": "typo3cms@typo3.org",
		"role": "Developer"
	}],
	"support": {
		"chat": "https://typo3.org/help",
		"docs": "https://docs.typo3.org/c/contentblocks/content-blocks/main/en-us/",
		"issues": "https://forge.typo3.org",
		"source": "https://github.com/typo3/typo3"
	},
	"config": {
		"sort-packages": true
	},
	"require": {
		"symfony/var-exporter": "^7.0",
		"typo3/cms-backend": "13.2.*@dev",
		"typo3/cms-core": "13.2.*@dev",
		"typo3/cms-fluid": "13.2.*@dev",
		"typo3/cms-frontend": "13.2.*@dev"
	},
	"conflict": {
		"typo3/cms": "*"
	},
	"extra": {
		"branch-alias": {
			"dev-main": "13.2.x-dev"
		},
		"typo3/cms": {
			"extension-key": "content_blocks",
			"Package": {
				"serviceProvider": "TYPO3\\CMS\\ContentBlocks\\ServiceProvider",
				"partOfFactoryDefault": false
			}
		}
	},
	"autoload": {
		"psr-4": {
			"TYPO3\\CMS\\ContentBlocks\\": "Classes/"
		}
	}
}
